
menuconfig MCOM03_PINCTRL
	bool "MCom-03 pin controller drivers"
	depends on PINCTRL && ARCH_MCOM03

if MCOM03_PINCTRL

config PINCTRL_MCOM03_HSPERIPH
	bool "Pin controller driver for HSPERIPH subsystem"
	default y
	select GENERIC_PINCONF
	help
	  This selects the pinctrl driver for HSPERIPH subsystem to configure
	  EMAC0/1, SDMMC0/1, NAND, QSPI1, USB0/1 controllers.

endif
